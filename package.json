{
  "name": "waves",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "node node_modules/react-native/local-cli/cli.js start",
    "test": "jest",
    "test:lint": "eslint . --quiet",
    "test:e2e:debug": "detox test -c ios.sim.debug"
  },
  "dependencies": {
    "react": "16.2.0",
    "react-native": "0.53.3",
    "react-navigation": "^1.2.1",
    "react-navigation-redux-helpers": "^1.0.1",
    "react-redux": "^5.0.7",
    "redux": "^3.7.2"
  },
  "devDependencies": {
    "babel-eslint": "^8.2.2",
    "babel-jest": "22.4.1",
    "babel-preset-react-native": "4.0.0",
    "detox": "^7.1.0",
    "eslint": "^4.18.1",
    "eslint-config-universe": "^1.0.7",
    "eslint-plugin-detox": "^1.0.0",
    "jest": "22.4.2",
    "prettier": "^1.11.1",
    "react-test-renderer": "16.2.0"
  },
  "jest": {
    "preset": "react-native"
  },
  "detox": {
    "test-runner": "jest",
    "runner-config": "e2e/config.json",
    "configurations": {
      "ios.sim.debug": {
        "binaryPath": "ios/build/Build/Products/Debug-iphonesimulator/waves.app",
        "build": "xcodebuild -project ios/waves.xcodeproj -scheme waves -configuration Debug -sdk iphonesimulator -derivedDataPath ios/build",
        "type": "ios.simulator",
        "name": "iPhone 6"
      }
    }
  }
}
